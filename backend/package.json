{
  "name": "secondbrain-backend",
  "version": "1.0.0",
  "description": "A backend API for SecondBrain application with AI-powered content management, vector search, and intelligent content parsing",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:frontend": "cd ../frontend && npm run build",
    "copy:frontend": "npm run clean:frontend && copyfiles -u 1 \"../frontend/dist/**/*\" dist/frontend",
    "build:full": "npm run build:frontend && npm run copy:frontend && npm run build",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "watch": "tsc -w",
    "start:dev": "nodemon dist/index.js",
    "start:prod": "NODE_ENV=production npm run start",
    "clean": "rimraf dist",
    "clean:frontend": "rimraf dist/frontend",
    "clean:all": "rimraf dist build",
    "rebuild": "npm run clean:all && npm run build:full",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify:build": "echo \"Verifying build structure...\" && ls -la dist/ && echo \"Build verification complete.\""
  },
  "keywords": [
    "secondbrain",
    "ai",
    "vector-search",
    "content-management",
    "api",
    "typescript",
    "express"
  ],
  "author": "SecondBrain Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@qdrant/js-client-rest": "^1.13.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.0.3",
    "@types/puppeteer": "^5.4.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "echobrain-ai-powered-second-brain": "file:..",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.1",
    "puppeteer": "^24.1.0",
    "typescript": "^5.6.3",
    "uuid": "^11.0.5",
    "youtube-transcript": "^1.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}